{
  "tabnine.experimentalAutoImports": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "liveServer.settings.CustomBrowser": "chrome",
  // setting vim
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "files.autoSave": "onFocusChange",
  "vim.sneak": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 5,
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "100",
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggestFontSize": 14,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": false,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "w"],
      "commands": [":w", "<cr>"]
    },
    {
      "before": ["<leader>", "a"],
      "after": ["A"]
    },
    {
      "before": ["<leader>", "{"],
      "after": ["[", "{"]
    },
    {
      "before": ["<leader>", "}"],
      "after": ["]", "}"]
    },

    {
      "before": ["<leader>", "("],
      "after": ["[", "("]
    },
    {
      "before": ["<leader>", ")"],
      "after": ["]", ")"]
    },
    {
      "before": ["<leader>", "i"],
      "after": ["I"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["revealInExplorer"]
    },

    {
      "before": ["<leader>", "h"],
      "after": ["_"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["$"]
    },
    {
      "before": ["<leader>", "r", "f"],
      "commands": ["fileutils.renameFile"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    /* move line down */
    {
      "before": ["<leader>", "n"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<leader>", "o", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "o", "o"],
      "commands": ["workbench.action.showEditorsInActiveGroup"]
    },
    {
      "before": ["<leader>", "o", "g"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["Z", "Z"],
      "commands": [":q!"]
    },
    {
      "before": ["<space>", "q"],
      "after": [":", "q", "!", "<Enter>"]
    },
    {
      "before": ["<Space>", "s", "v"],
      "commands": [":vsplit"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["1"],
      "after": ["^"]
    },
    {
      "before": ["g", "l"],
      "after": ["g", "t"]
    },
    // { "before": ["<S-h>"], "commands": [":bprevious"] },
    // { "before": ["<S-l>"], "commands": [":bnext"] },
    {
      "before": ["leader", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["leader", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["leader", "l", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["g", "b"],
      "after": ["<C-t>"]
    },
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    }
  ],
  "vim.operatorPendingModeKeyBindingsNonRecursive": [
    {
      "before": ["("],
      "after": ["i("]
    },

    {
      "key": "ctrl+l",
      "command": "workbench.action.focusRightGroup",
      "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
    },
    {
      "key": "ctrl+k",
      "command": "selectPrevSuggestion",
      "when": "suggestWidgetVisible"
    }
  ],
  "vim.visualModeKeyBindings": [
    // Stay in visual mode while indenting
    { "before": ["<"], "commands": ["editor.action.outdentLines"] },
    { "before": [">"], "commands": ["editor.action.indentLines"] },
    // Move selected lines while staying in visual mode
    { "before": ["J"], "commands": ["editor.action.moveLinesDownAction"] },
    { "before": ["K"], "commands": ["editor.action.moveLinesUpAction"] }
    // toggle comment selection
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-d>": false,
    "<C-j>": false,
    "<C-c>": false,
    "<C-s>": false,
    "<C-x>": false,
    "<C-v>": false,
    "<C-k>": false,

    "<C-b>": false,
    "<C-o>": false,
    "<C-p>": false
  },
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#3a6680", "#ffff"],
  "vim.statusBarColors.insert": "#1a0232",
  "vim.statusBarColors.visual": "#B48E49",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-icons",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#ff5555",
    "panelTitle.activeBorder": "#f1fa8c",
    "panelTitle.activeForeground": "#f1fa8c",
    "panelTitle.inactiveForeground": "#CCCCCC",
    "minimap.selectionHighlight": "#50fa7b",
    "minimap.errorHighlight": "#ff5555",
    "minimap.warningHighlight": "#f1fa8c",
    "minimap.background": "#11101023",
    "minimap.findMatchHighlight": "#601bc2",
    "minimapSlider.background": "#D6C2C223",
    "editorLineNumber.activeForeground": "#50fa7b",
    "editorLineNumber.foreground": "#888",
    "statusBar.border": "#3B3C40",
    "statusBar.foreground": "#ffff",
    "tab.activeBackground": "#D6C2C223",
    "tab.activeBorder": "#ff5555",
    "tab.activeBorderTop": "#3B3C40",
    "tab.activeModifiedBorder": "#50fa7b",
    "titleBar.activeForeground": "#f1fa8c",
    "titleBar.inactiveForeground": "#d2d3d8",
    "titleBar.border": "#3B3C40",
    "sideBar.border": "#3B3C40",
    "sideBarTitle.foreground": "#ff79c6",
    "sideBar.foreground": "#f8f8f2",
    "sideBarSectionHeader.foreground": "#ebeae8",
    "activityBar.foreground": "#50fa7b",
    "activityBar.inactiveForeground": "#f8f8f2",
    "icon.foreground": "#f1fa8c",
    "breadcrumb.foreground": "#50fa7b",
    "terminal.background": "#242323",
    "terminal.foreground": "#DDDDDD",
    "terminalCursor.foreground": "#50fa7b",
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564",
    "editor.wordHighlightBorder": "#f1fa8c",
    "editor.wordHighlightStrongBorder": "#50fa7b",
    "editorBracketHighlight.foreground1": "#ff79c6",
    "editorBracketHighlight.foreground2": "#ffb86c",
    "editorBracketHighlight.foreground3": "#f1fa8c",
    "editorBracketHighlight.foreground4": "#50fa7b",
    "editorBracketHighlight.foreground5": "#8be9fd",
    "editorBracketHighlight.foreground6": "#bd93f9",
    "editorError.foreground": "#ff5555",
    "editorWarning.foreground": "#f1fa8c",
    "tree.indentGuidesStroke": "#50fa7b",
    "button.background": "#717475",
    "button.foreground": "#f1fa8c",
    "statusBar.background": "#B48E49",
    "statusBar.noFolderBackground": "#B48E49",
    "statusBar.debuggingBackground": "#B48E49",
    "statusBar.debuggingForeground": "#ffff"
  },
  "terminal.integrated.env.linux": {},
  "workbench.colorTheme": "Dracula",
  "editor.fontFamily": "'Iosevka Nerd Font Mono', 'monospace', monospace",
  "window.menuBarVisibility": "toggle",
  "dotnet.server.useOmnisharp": true,
  "glassit.alpha": 220,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": -1,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "[handlebars]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  }
}
